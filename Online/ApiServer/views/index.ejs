    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h4 class="page-header">AttackTags 
                    <medium>- Real time threat intelligence</medium>
                </h4>                
            </div>
        </div>
    </div>
    <!-- /.container -->

    <!-- Page Content -->
    <div class="container titlepadding" id="breakouts">

        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h4 class="page-header">Hot
                    <small>- Most popular Hashtags</small>
                </h4>
            </div>
        </div>
        <!-- /.row -->
	<% for (var i = 0; i < hashtagsWithTweets.length; ++i) {%>
	<%     var shouldRender = false; %>
	<%     for (var index = 0; !shouldRender && index < hashtagsWithTweets[i].tweets.length; ++index) {%>
	<%         if (hashtagsWithTweets[i].tweets[index] && hashtagsWithTweets[i].tweets[index][0]) {%>
	<%             shouldRender = true;%>
	<%         }%>
	<%     }%>
	<%     if (shouldRender) {%>
        <!-- one hashtag -->
        <div class="row">
            <div class="col-md-2">
                <a href="#">
                  <!--5
                    <img class="img-responsive" src="http://placehold.it/120x120" alt="">
                  -->
                  <h4>#<%=hashtagsWithTweets[i].hashtag%></h4>
                </a>
            </div>
            <div class="col-md-9">
		<% hashtagsWithTweets[i].tweets.forEach(function print(tweet) { %>
		<%     if (tweet && tweet[0]) { %>
                <p class="col-md-9"><%=tweet[0].text%></p>
                <a class="btn btn-xs btn-primary pull-right" href="<%=tweet[0].link%>">View 
			<span class="glyphicon glyphicon-chevron-right">
			</span>
		</a>
		<%     }%>
                <% }) %>
            </div>
        </div>
	<%    }%>
	<% }%>
        <!-- /.row -->
        <hr>
    </div>
    <!-- /.container -->

    <!-- Page Content -->
    <div class="container titlepadding" id="news">

        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h4 class="page-header">News 
                    <small>- General Security News</small>
                </h4>
            </div>
        </div>
        <!-- /.row -->

        <!-- /.row -->

        <% for (var i = 0; i < tweetsWithImg.tweetsWithUrl.length && i < n; ++i) {%>
        <div class="row">
            <div class="col-md-2">
                <a href="<%=tweetsWithImg.tweetsWithUrl[i].link%>" target="_blank">
                    <img height="120" width="120" class="img-responsive" src="<%=tweetsWithImg.imgList[i].unescapedUrl%>" alt="">
                </a>
            </div>
            <div class="col-md-9">
                <h4><%=tweetsWithImg.tweetsWithUrl[i].text%></h4>
                <a class="btn btn-primary" href="<%=tweetsWithImg.tweetsWithUrl[i].link%>" target="_blank">
                    View News<span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
        </div>
        <!-- /.row -->

        <hr>
        
        <% } %>

        <hr>
    </div>
    <!-- /.container -->

    <!-- Page Content -->
